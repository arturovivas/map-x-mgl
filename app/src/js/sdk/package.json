{
  "name": "@fxi/mxsdk",
  "version": "1.6.97-beta",
  "description": "sdk for mapx app",
  "source": "src/index.js",
  "main": "dist/mxsdk.js",
  "module": "dist/mxsdk.mjs",
  "unpkg": "dist/mxsdk.umd.js",
  "browser": "dist/mxsdk.umd.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "microbundle -o dist --no-compress && live-server tests --mount=/dist:dist --mount=/data:data",
    "dev": "microbundle watch -o dist --no-compress",
    "dev-live": "live-server dev --mount=/dist:dist --mount=/data:data",
    "examples": "microbundle -o dist --no-compress && live-server examples --mount=/dist:dist",
    "prod": "microbundle -o dist && npm run jsdoc && npm publish",
    "jsdoc": "jsdoc2md --template readme.hbs --files src/**/*.js > readme.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unep-grid/map-x-mgl.git"
  },
  "keywords": [
    "mapx",
    "sdk"
  ],
  "author": "Fred Moser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unep-grid/map-x-mgl/issues"
  },
  "homepage": "https://github.com/unep-grid/map-x-mgl/app/src/js/sdk/#readme",
  "devDependencies": {
    "jsdoc-to-markdown": "^5.0.3",
    "live-server": "^1.2.1",
    "microbundle": "^0.12.0"
  },
  "dependencies": {
    "@fxi/el": "0.0.12"
  }
}
